{
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "no-unused-vars": "warn",
      "no-undef": "warn",
      "vue/no-unused-components": "warn",
      "prefer-const": "warn"
    }
}
